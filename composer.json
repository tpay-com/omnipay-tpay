{
    "name": "omnipay/tpay",
    "description": "Tpay gateway for Omnipay payment processing library",
    "license": "MIT",
    "type": "library",
    "version": "v1.0.0",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "tpay",
        "tpay.com",
        "purchase"
    ],
    "authors": [
        {
            "name": "Tpay.com",
            "email": "pt@tpay.com"
        },
        {
            "name": "Tpay contributors",
            "homepage": "https://github.com/"
        }
    ],
    "homepage": "https://github.com/",
    "require": {
        "omnipay/common": "^2.0 || ^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^2.0 || ^3.0 || ^4.0",
        "php-parallel-lint/php-parallel-lint": "^1.3.2"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Tpay\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
